<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="GestureView">
        <attr name="gv_outer_radius" format="reference|integer" />
        <attr name="gv_outer_color" format="reference|integer" />
        <attr name="gv_inner_radius" format="reference|color" />
        <attr name="gv_inner_color" format="reference|color" />
        <attr name="gv_line_color" format="reference|color" />
        <attr name="gv_line_width" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="GestureResultView">
        <!--半径-->
        <attr name="grv_radius" format="integer|reference" />
        <!--未选中颜色-->
        <attr name="grv_color_normal" format="color|reference" />
        <!--选中颜色-->
        <attr name="grv_color_select" format="color|reference" />
    </declare-styleable>

</resources>